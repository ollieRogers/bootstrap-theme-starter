          .row
            .col-sm-2.col-xs-4
              // Lead body copy
              span.lead .lead
              br
              // Bold
              b b (bold)
              br
              strong strong
            .col-sm-2.col-xs-4
              // Small text
              small small
              br
              span.small .small
            .col-sm-2.col-xs-4
              // Italics
              em em
              br
              i i (italic)
            // Inline text elements
            .col-sm-2.col-xs-4
              // Deleted text
              del del
              br
              // Strikethrough text
              s s (strikethrough)
              br
              // Inserted text
              ins ins
              br
              // Underlined text
              u u (underline)
            .col-sm-2.col-xs-4
              // Inline code
              code code
              br
              // Variables
              var var
              br
              // Sample output from a program
              samp samp
            .col-sm-2.col-xs-4
              // Marked text
              mark mark
              br
              // User input
              kbd kbd
          br
          // Alignment classes
          .row
            .col-xs-4
              p.text-left .text-left
            .col-xs-4
              p.text-center .text-center
            .col-xs-4
              p.text-right .text-right
            .col-xs-12
              .text-justify .text-justify
          br
          // Transformation classes
          
          span.text-lowercase .text-lowercase
          span.text-uppercase .text-uppercase
          span.text-capitalize .text-capitalize
          br
          // Abbreviations
         
          abbr(title='abbreviation') abbr
          abbr.initialism(title='abbreviation.initialism')
              | .initialism
          br
          // Addresses

          address
              strong address strong
              br
              | address
              br
              a(href='mailto:#') first.last@example.com
