          .row
            .col-sm-6
              blockquote
                p blockquote p
                footer
                  | blockquote footer
                  cite(title='Cite') cite
            .col-sm-6
              blockquote.blockquote-reverse
                p blockquote.blockquote-reverse p
                footer
                  | Someone famous in
                  cite(title='Source Title') Source Title
